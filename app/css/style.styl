	//font-size 10px
	text-align center
.exc-table-el-text
	+below(640px)
		flex 1 1 100%
		text-align left
		font-size 12px
		margin 10px 0
		flexB(mid)
		p
			width 100%
			padding-left 20%
	flex 4
	font-size 14px
	flexB(mid)
	justify-content flex-start
	fw()
	.fa
		color blue
		margin-right 10px
	p
		margin 0
		margin-bottom 5px
		margin-right 5px
.exc-table-el-cost
	+below(768px)
		text-align left
	+below(640px)
		flex 1 1 100%
	color black
	flex 1
	font-size 18px
	font-weight 600
.exc-table-el-right
	+below(768px)
		flex 1 1 100%
	flex 1
.exc-table-el-cost-val
	text-align center
	font-size 22px
.exc-table-el-cost-date
	text-align center
	font-size 12px
	margin-top 10px

.exc-list-goodprice 
	position relative
	strong
		color red
.exc-list-sale
	+below(768px)
		left 0px
	position absolute
	left 10px
	top 10px
	color red
	font-weight 600
.exc-list-badprice 
	position relative
	color black!important
	+below(768px)
		&:before
			content ''
			margin-top -2px
.exc-list-badprice-line
	&:before
		content ''
		height 2px
		width 100%
		position absolute
		left 0px
		top 50%
		background red
	
@-webkit-keyframes rotating 
	from
		webkit-transform: rotate(0deg);
	to
		-webkit-transform: rotate(360deg);


.exc-list-price-val img
	-webkit-animation: rotating .6s linear infinite;
.exc-list-content.exc-list-goodprice img
	-webkit-animation: rotating .6s linear infinite;
	width 20px
	height 20px
	margin 0 auto
.jq-toast-single
	background white 
	color black
	bsh()
	border: 1px solid #dcd7d7;
.jq-toast-heading
	color white
.result-right-footer
	flexB(around)
	fw()
	padding 20px 0
	a
		color blue
		width 30px
		height 30px
		border-radius 50%
		border 1px solid blue
		flexB(mid)
		tr(all)
		&:hover
			background blue
			color white 
.trevel-message
	padding 5px
	f()
	border-radius 4px
	background #f7f7f7
	margin-bottom 10px
	img
		display block 
		width 40px !important
		height 40px !important
		border-radius 4px
		margin-right 10px
.trevel-message-name
	font-size 12px
	margin-bottom 5px
	font-weight 600 
.trevel-message-text
	font-size 12px

.baner-sale
	+below(768px)
		margin-left -20px
	margin-left -35px
	background red
	padding 5px
	border-radius 4px 4px 4px 0px
	width 90px
	flexB(mid) 
	fl()
	margin-bottom 10px
	position relative
.baner-sale-title
	color white
.baner-sale-val
	color white
	font-size 20px
	font-weight 700 
.baner-sale-coner
	+below(768px)
		display none
	position absolute
	width 0
	height 0
	border-top 10px solid #9e0812
	border-left 15px solid transparent
	left 0px
	bottom -10px

.search-res
	position absolute
	top 100%
	z-index 2000
	background white 
	width 100%
	display none
	box-shadow  0px 1px 3px grey
.search-res-row
	padding 10px
	padding-left 30px
	display block
	position relative
	overflow-x hidden
	&:hover
		background blue
		color white
		.search-res-info
			color white
.search-res-info
	flexB(between)
	color black
.search-res-name
	.fa
		position absolute
		left 10px
		top 10px
		color blue
.search-res-current
	font-weight 700
	display inline
.search-res-val
	color grey
	font-size 10px
	white-space nowrap
	margin-left 10px
